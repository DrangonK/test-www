body {
    background: white!important;
}
.fl{
    float: left;
}
.cblue{
    color: #004098 !important;
}
#app{
    display: none;
}
body #compatibility{
    width: 1130px;
    margin: 160px auto 0;
    display: block!important;
}
body #compatibility .pic{
    margin: 35px 60px 0;
}

body #compatibility .text h3{
    font-size: 24px;
    font-weight: normal;
    margin-top: 85px;
    margin-bottom: 30px;
}
body #compatibility .text p{
    line-height: 26px;
    padding-bottom: 35px;
    margin-bottom: 30px;
    border-bottom: 1px solid #a8b1ba;
}
body #compatibility .text .browser{
    background: url('/public/images/compatibility02.jpg') no-repeat;
    width: 410px;
    height: 44px;
    margin: auto;
}
body #compatibility .text .browser a{
    float: left;
    display: block;
    width: 57px;
    height: 44px;
    margin-right: 24px;
}
