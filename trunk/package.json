    "@types/lowdb": "^1.0.5",
    "@types/mocha": "^2.2.40",
    "@types/node": "^7.0.12",
    "@types/qs": "^6.5.1",
    "@types/supertest": "^2.0.0",
    "@vue/cli-plugin-babel": "^3.0.3",
    "@vue/cli-plugin-typescript": "^3.0.3",
    "@vue/cli-service": "^3.0.3",
    "autod": "^3.0.1",
    "autod-egg": "^1.1.0",
    "axios": "^0.18.0",
    "copy-webpack-plugin": "^4.6.0",
    "egg-bin": "^4.6.2",
    "egg-ci": "^1.8.0",
    "egg-logview": "^2.0.0",
    "egg-mock": "^3.16.0",
    "egg-ts-helper": "^1.11.0",
    "element-ui": "2.4.4",
    "js-cookie": "2.2.0",
    "lowdb": "^1.0.0",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "tslib": "^1.9.0",
    "tslint": "^4.0.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.0.0",
    "vue": "^2.5.17",
    "vue-class-component": "^6.0.0",
    "vue-cropper": "^0.2.9",
    "vue-property-decorator": "^7.0.0",
